<h1>Songs</h1>

<table>
  <thread>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Artist name</th>
      <th>Genre</th>
      <th></th>
    </tr>
  </thread>

<tbody>
<% @songs.each do |song| %>
<tr>
<td><%= song.title %></td>
<td><%= song.artist_name%></td>
<td><%= song.genre%></td>
<td><%= song.release_year%></td>
<td><%= song.released%></td>
<td>
  <%= link_to 'View', song_path(song) %>
  <%= link_to 'Edit', edit_song_path(song) %>
  <%= link_to 'Delete', song, method: :delete, data: {confirm: 'For real?' }%>
</td>
</tr>
  <% end %>
</tbody>
</table>
